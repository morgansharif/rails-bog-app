<div class="col-md-8 col-md-offset-2">
  <%= form_for @creature do |f| %>
  <strong>Name:</strong><br />
  <%= f.text_field :name %><br />
  <strong>Description</strong>:<br />
  <%= f.text_area :description %><br />
  <%= f.submit "Save New Creature", class: "btn btn-primary" %>
  <% end %>
</div>
